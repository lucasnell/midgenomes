# raxml.sub

# Create phylogeny using RAxML-NG

universe = docker
docker_image = lucasnell/midgenomes:v1.0.8

log = raxml.log
error = raxml.err
output = raxml.out

executable = raxml.sh

batch_name = "raxml"

should_transfer_files = YES
when_to_transfer_output = ON_EXIT

requirements = (Target.HasCHTCStaging == true)

request_cpus = 48
request_memory = 12GB
request_disk = 20GB

queue 1
