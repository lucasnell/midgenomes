# blobtk.sub

# Analyze Tanytarsus gracilentus genome using BlobToolKit

universe = docker
docker_image = genomehubs/blobtoolkit:3.2.6

log = blobtk.log

batch_name = "blobtk"

should_transfer_files = YES
when_to_transfer_output = ON_EXIT

requirements = (Target.HasCHTCStaging == true)

request_cpus = 32
request_memory = 32GB
request_disk = 200GB

queue 1
